{
  "name": "vue-simple-svg",
  "version": "2.0.2",
  "description": "A simple Vue.js plugin that allows you to use a component that loads a SVG image as an inline SVG so you can easily control its fill color from the parent component.",
  "main": "dist/plugin.js",
  "scripts": {
    "rollup": "rollup --config rollup.config.js",
    "dev-demo": "webpack serve --progress --config demo/build/webpack.dev.conf.js",
    "start": "npm run dev-demo",
    "build-demo": "node demo/build/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seiyable/vue-simple-svg.git"
  },
  "keywords": [
    "vue svg",
    "vuejs svg",
    "vue.js svg",
    "vue inline svg"
  ],
  "author": "Seiya Kobayashi",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.7",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.5",
    "babel-runtime": "^6.26.0",
    "chalk": "^4.1.2",
    "chromedriver": "^102.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "css-loader": "^6.7.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-notifier": "^5.2.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "ora": "^5.4.1",
    "portfinder": "^1.0.28",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "reset.css": "^2.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-uglify": "^6.0.4",
    "selenium-server": "^3.141.59",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
