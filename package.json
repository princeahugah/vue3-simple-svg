    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "cssom": "^0.5.0"
  },
  "engines": {
    "yarn": "1.x",
    "npm": "8.x",
    "node": "16.x"
  }
}
